<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ic_base_log.handler.amqp_handler_class">IC\Bundle\Base\LogBundle\Handler\RabbitMqHandler</parameter>
    </parameters>


    <services>

        <service id="ic_base_log.handler.amqp_handler"
                 class="%ic_base_log.handler.amqp_handler_class%">
            <call method="setProducer">
                <argument type="service" id="old_sound_rabbit_mq.log_producer"/>
            </call>
        </service>

    </services>

</container>
