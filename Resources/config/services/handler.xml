<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ic_base_log.handler.amqp_handler_class">Monolog\Handler\AmqpHandler</parameter>
    </parameters>

    <services>
        <service id="ic_base_log.handler.amqp_handler" class="%ic_base_log.handler.amqp_handler_class%">
            <argument type="service" id="ic_base_amqp.exchange.affiliate_log" />
        </service>
    </services>

</container>
